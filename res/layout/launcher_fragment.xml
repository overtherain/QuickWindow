<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="fill_parent"
    android:layout_height="fill_parent"
    android:background="@android:color/transparent" >

    <com.cg.quickcircle.CircleLayout
        android:id="@id/launcher_circle_layout"
        android:layout_width="fill_parent"
        android:layout_height="fill_parent"
        android:layout_gravity="center_horizontal" >

        <com.cg.quickcircle.CircleImageView
            android:id="@id/main_phone_image"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:background="@drawable/ic_launcher_call_log" />

        <com.cg.quickcircle.CircleImageView
            android:id="@id/main_sms_image"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:background="@drawable/ic_launcher_message" />

        <com.cg.quickcircle.CircleImageView
            android:id="@id/main_camera_image"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:background="@drawable/ic_launcher_camera" />

        <com.cg.quickcircle.CircleImageView
            android:id="@id/main_music_image"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:background="@drawable/ic_launcher_music" />

        <com.cg.quickcircle.CircleImageView
            android:id="@id/main_setting_image"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:background="@drawable/ic_launcher_setting" />
    </com.cg.quickcircle.CircleLayout>

</RelativeLayout>
